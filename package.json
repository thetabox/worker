{
	"name": "thetabox-worker",
	"version": "0.0.1",
	"main": "lib/server.js",
	"typings": "lib/server.d.ts",
	"private": true,
	"license": "thetabox",
	"scripts": {
		"start": "npm run build && env-cmd -f .env node lib/server.js",
		"dev": "kill-port 4444 && env-cmd -f .env nodemon",
		"test": "jest --watchAll --forceExit --detectOpenHandles",
		"build": "tsc -p .",
		"build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/server.ts",
		"build:export": "nodemon",
		"update": "ncu -u @theta*/* && npm install",
		"docker": "docker build -t thetabox/thetabox-worker:0.0.1 -f ./Dockerfile . --label 'fix=init'",
		"run": "docker run --rm -it  thetabox/thetabox-worker:0.0.1",
		"push": "docker push thetabox/thetabox-worker:0.0.1",
		"deploy": "npm run build && npm run docker && npm run push"
	},
	"dependencies": {
		"@google-cloud/firestore": "^5.0.2",
		"@google-cloud/logging-bunyan": "^3.3.0",
		"@thetabox/model": "^0.0.42",
		"@thetabox/services": "^0.0.18",
		"@thetanext/model": "^0.0.6",
		"@thetanext/services": "^0.0.5",
		"@types/fluent-ffmpeg": "^2.1.20",
		"@types/fs-extra": "^9.0.13",
		"@types/node": "^17.0.24",
		"axios": "^0.26.1",
		"cors": "^2.8.5",
		"date-fns": "^2.28.0",
		"dotenv": "^16.0.0",
		"env-cmd": "^10.1.0",
		"express": "^4.17.3",
		"fluent-ffmpeg": "^2.1.2",
		"fs-extra": "^10.1.0",
		"numbro": "^2.3.6",
		"p-queue": "^7.2.0",
		"typescript": "^4.6.3"
	},
	"devDependencies": {
		"@types/jest": "^27.4.1",
		"@types/supertest": "^2.0.12",
		"jest": "^27.5.1",
		"jest-watch-typeahead": "^1.0.0",
		"kill-port": "^1.6.1",
		"nodemon": "^2.0.15",
		"npm-check-updates": "^12.5.9",
		"prettier": "^2.6.2",
		"supertest": "^6.2.2",
		"ts-jest": "^27.1.4",
		"ts-node": "^10.7.0"
	}
}